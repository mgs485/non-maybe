// @flow

function nonMaybe<T>(value: T): $NonMaybeType<T> {
  if (global?.expect) {
    expect(value).toBeTruthy()
  }

  if (value === null || value === undefined) {
    const e = new Error('NonMaybeType cast failed.')
    throw new Error(e.stack.toString())
  }
  return value
}

export default nonMaybe